{
  "name": "@zougui/firestone-bot",
  "private": true,
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "start:controller": "node --enable-source-maps dist kill",
    "start:bot": "node --enable-source-maps dist bot",
    "start:server": "uvicorn server.server:app --host 127.0.0.1 --port 8000",
    "compile": "tsc --project .",
    "compile:watch": "npm run compile -- --watch",
    "clean": "rimraf ./dist & rimraf ./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "build": "npm run clean && npm run compile",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@effect/opentelemetry": "^0.45.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "1.25",
    "@opentelemetry/sdk-logs": "^0.200.0",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@opentelemetry/sdk-trace-node": "1.25",
    "@opentelemetry/sdk-trace-web": "1.25",
    "@xstate/store": "^3.2.0",
    "axios": "^1.8.1",
    "big.js": "^6.2.2",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "effect": "^3.14.2",
    "emittery": "^0.13.1",
    "env-var": "^7.5.0",
    "execa": "^9.5.2",
    "fast-levenshtein": "^3.0.0",
    "immer": "^10.1.1",
    "mongodb": "^6.15.0",
    "nanoid": "^2.1.11",
    "node-moving-things-tracker": "^0.9.1",
    "papr": "^15.2.2",
    "radash": "^12.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "^22.13.9",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "type-fest": "^4.39.1",
    "typescript": "~5.6.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "iohook",
      "robotjs",
      "tesseract.js"
    ],
    "onlyBuiltDependencies": [
      "iohook",
      "robotjs",
      "tesseract.js"
    ]
  }
}
